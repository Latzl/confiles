# @synopsis: pre-msg
# @description: goto previous massage

;WITH lines_previous AS (
    SELECT 
        CAST(log_line AS INTEGER) AS log_line 
    FROM 
        all_logs
    WHERE 
        CAST(log_line AS INTEGER) < log_top_line()
)
SELECT COALESCE(MAX(log_line), 0) AS pre_line FROM lines_previous;

:eval :goto ${pre_line}
